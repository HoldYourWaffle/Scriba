{
	"compilerOptions": {
		"target": "es5",
		"outDir": "./dist",
		"sourceMap": true,
		"incremental": true,
		"lib": [ "ES2015", "ES2018", "ES2019", "DOM" ],
		
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		
		"module": "CommonJS",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true
		// "allowUmdGlobalAccess": true,
	},
	
	"include": [ "src" ]
}
